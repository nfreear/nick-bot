    "nlp": {
      "corpora": [
        "./corpus-en.json",
        "./corpus-es.json"
      ]
    },
    "ner": {
      "threshold": 0.8
    },
    "console": {
      "debug": true
    },
    "api-server": {
      "port": 3000,
      "serveBot": false
    }
  },
  "use": ["Basic", "LangEs", "ConsoleConnector", "ExpressApiServer", "DirectlineConnector"]
}
