{
  "private": true,
  "name": "nick-bot",
  "version": "0.8.1",
  "description": "An experimental Chat-bot, built on `NLP.js`",
  "license": "GPL-3.0+",
  "X-source": "https://github.com/jesus-seijas-sp/nlpjs-examples/tree/master/01.quickstart/08.webchat",
  "main": "./index.js",
  "scripts": {
    "start": "node index.js",
    "frontend": "live-server --port=3001 --no-browser --watch=public --open='/public/'",
    "frontend:zh": "live-server --port=3001 --watch=public --open='/public/?lang=zh-CN'",
    "lint:fix": "semistandard --fix",
    "vox:list": "say -v ? | egrep '(en|zh)_'",
    "test": "semistandard"
  },
  "author": "Nick Freear",
  "repository": "https://github.com/nfreear/nick-bot.git",
  "dependencies": {
    "@nlpjs/basic": "^4.1.4",
    "@nlpjs/directline-connector": "^4.1.4",
    "@nlpjs/express-api-server": "^4.1.4",
    "@nlpjs/lang-es": "^4.1.4",
    "@nlpjs/lang-zh": "^4.2.1",
    "@nlpjs/ner": "^4.1.4",
    "xregexp": "^4.3.0"
  },
  "devDependencies": {
    "live-server": "^1.2.1",
    "semistandard": "^14.2.0"
  },
  "peerDependencies": {
    "botframework-webchat-root": "https://github.com/microsoft/BotFramework-WebChat.git#v4.7.1"
  }
}
